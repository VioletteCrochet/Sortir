{% form_theme eventForm 'tailwind_2_layout.html.twig' %}
{% form_theme addressForm 'tailwind_2_layout.html.twig' %}

<div class="w-1/3">


{{ form_start(eventForm) }}
    {{ form_widget(eventForm) }}
    {{ form_row(addressForm.name) }}
    {{ form_row(addressForm.city) }}
    <ul id="suggestions-list" class="absolute w-full bg-white border border-gray-300 rounded-md shadow-md max-h-52 overflow-y-auto hidden"></ul>
    {{ form_widget(addressForm) }}
    <button class="btn">{{ button_label|default('Save') }}</button>
{{ form_end(eventForm) }}
</div>
<script src="{{ asset('js/addressAutocomplete.js') }}"></script>
